type Student {
    studentId: ID!
    firstName: String!
    middleName: String
    lastName: String!
    sex: String
    dob: String!
    email: String!
    ohioSsid: String!
    dailyAttendance: [DailyAttendance]
}

extend type Query {
    allStudents: [Student]
    studentById(studentId: ID!): Student
    studentsByLastName(lastName: String!): [Student]
}

